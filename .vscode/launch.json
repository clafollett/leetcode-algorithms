    "version": "0.2.0",
    "configurations": [
        {
            "name": "CSharp 001 - Easy - TwoSum",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/LeetCode.Algorithms.dll",
            "args": ["two-sum", "--nums", "4,13,19,45,179,192,253,284,429,1287,2947", "--target", "474"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Rust - Debug",
            "preLaunchTask": "rust: build",
            "program": "${fileDirname}${pathSeparator}target${pathSeparator}${fileBasenameNoExtension}",
            "args": [],
            "cwd": "${fileDirname}",
        }

    ]
}